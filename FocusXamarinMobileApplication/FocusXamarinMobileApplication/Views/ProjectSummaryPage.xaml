<?xml version="1.0" encoding="UTF-8"?><ContentPage xmlns="http://xamarin.com/schemas/2014/forms"             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"             NavigationPage.HasNavigationBar="false"             x:Class="FocusXamarinForms20082020V1.Views.ProjectSummaryPage">    <ContentPage.Resources>        <ResourceDictionary Source="StyleResource.xaml" />    </ContentPage.Resources>    <ContentPage.Content>        <FlexLayout            Direction="Column"            BackgroundColor="#292F62"            FlexLayout.Basis="100%"            Padding="10">            <StackLayout Orientation="Vertical" HorizontalOptions="FillAndExpand">                <StackLayout Orientation="Horizontal"                             Margin="2"                             FlexLayout.Basis="5%">                    <Label Text="{Binding ProjectName}"                           HorizontalOptions="Start"                           FontSize="Large"                           TextColor="White" />                    <Label Text="{Binding Date}"                           HorizontalOptions="EndAndExpand"                           FontSize="Large"                           TextColor="White" />                </StackLayout>                <StackLayout Orientation="Horizontal"                             Margin="2"                             FlexLayout.Basis="5%">                    <Button Text="Back"                            TextColor="Black"                            WidthRequest="100"                            HeightRequest="50"                            BackgroundColor="LightGray"                            HorizontalOptions="Start"                            Command="{Binding Back}" />                </StackLayout>                <StackLayout Orientation="Horizontal"                             Margin="2"                             FlexLayout.Basis="5%">                    <Label Text="{Binding UserName}"                           HorizontalOptions="CenterAndExpand"                           HeightRequest="20"                           FontSize="Large"                           TextColor="White" />                </StackLayout>                <!--StackLayout Orientation="Vertical" FlexLayout.Basis="12%">                    <Label Text="Commercial Summary" TextColor="White" />                    <Editor HorizontalOptions="FillAndExpand"                            VerticalOptions="FillAndExpand"                            HeightRequest="95"                            BackgroundColor="#1A142E"                            TextColor="White"                            IsReadOnly="True"                            Text="{Binding ProjectSummary.Commercial}" />                </StackLayout-->                <StackLayout Orientation="Vertical" FlexLayout.Basis="12%">                    <Label Text="Operational Summary" TextColor="White" />                    <Editor HorizontalOptions="FillAndExpand"                            VerticalOptions="FillAndExpand"                            HeightRequest="100"                            IsReadOnly="True"                            BackgroundColor="#1A142E"                            TextColor="White"                            Text="{Binding ProjectSummary.Operational}" />                </StackLayout>                <StackLayout Orientation="Vertical" FlexLayout.Basis="15%">                    <Label Text="SHEQ Summary" TextColor="White" />                    <Editor HorizontalOptions="FillAndExpand"                            VerticalOptions="FillAndExpand"                            HeightRequest="100"                            IsReadOnly="True"                            BackgroundColor="#1A142E"                            TextColor="White"                            Text="{Binding ProjectSummary.Sheq}" />                </StackLayout>                <StackLayout Orientation="Vertical" FlexLayout.Basis="12%">                    <Label Text="Supervisor Contact" TextColor="White" />                    <Editor HorizontalOptions="FillAndExpand"                            VerticalOptions="FillAndExpand"                            HeightRequest="100"                            BackgroundColor="#1A142E"                            TextColor="White"                            IsReadOnly="True" />                </StackLayout>                <StackLayout Orientation="Vertical" FlexLayout.Basis="12%">                    <Label Text="Nearest Hospital" TextColor="White" />                    <Editor                        HorizontalOptions="FillAndExpand"                        HeightRequest="100"                        FlexLayout.Basis="10%"                        IsReadOnly="True"                        Margin="5"                        VerticalOptions="FillAndExpand"                        BackgroundColor="#1A142E"                        TextColor="White"                        Text="{Binding HospitalInfo}" />                </StackLayout>                <Button Text="Get Directions"                        HorizontalOptions="FillAndExpand"                        HeightRequest="50"                        BackgroundColor="Green"                        TextColor="White"                        Command="{Binding OpenMapPage}" />            </StackLayout>        </FlexLayout>    </ContentPage.Content></ContentPage>