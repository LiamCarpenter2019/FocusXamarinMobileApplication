<?xml version="1.0" encoding="utf-8"?><ContentPage xmlns="http://xamarin.com/schemas/2014/forms"             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"             x:Class="FocusXamarinForms20082020V1.Views.ImageEditorPage"             NavigationPage.HasNavigationBar="false"             xmlns:imageeditor="clr-namespace:Syncfusion.SfImageEditor.XForms;assembly=Syncfusion.SfImageEditor.XForms"             xmlns:ios="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"             ios:Page.UseSafeArea="true">    <ContentPage.Content>        <FlexLayout Direction="Column" Padding="10" BackgroundColor="#292F62">            <StackLayout FlexLayout.Basis="95%" HorizontalOptions="FillAndExpand">                <Button HeightRequest="40"                        WidthRequest="100"                        HorizontalOptions="Start"                        Text="Back"                        BackgroundColor="LightGray"                        TextColor="Black"                        Command="{Binding Back}" />                <Frame HasShadow="True"                       BorderColor="DarkGray"                       CornerRadius="5"                       Margin="5"                       FlexLayout.Basis="100%"                       HorizontalOptions="FillAndExpand"                       VerticalOptions="FillAndExpand"                       BackgroundColor="#292F62">                    <imageeditor:SfImageEditor x:Name="imgEditor" WidthRequest="100" Source="{Binding Image}"                                               ImageSaving="ImageSaving" />                </Frame>            </StackLayout>        </FlexLayout>    </ContentPage.Content></ContentPage>